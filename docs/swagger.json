openapi: 3.1.0
info:
  title: Simple API
  version: 1.0.0

paths:
  /users:
    get:
      responses:
        200:
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/User'
          examples:
            - { id: 1, name: "John Doe", email: "johndoe@example.com" }
            - { id: 2, name: "Jane Smith", email: "janesmith@example.com" }

components:
  schemas:
    User:
      type: object
      properties:
        id:
          type: integer
        name:
          type: string
        email:
          type: string